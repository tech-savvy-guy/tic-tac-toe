{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "",
    "css": "app/globals.css",
    "baseColor": "slate",
    "cssVariables": true,
    "prefix": ""
  },
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
  "iconLibrary": "lucide",
  "registry": [
    {
      "name": "tic-tac-toe-game",
      "type": "registry:block",
      "title": "Tic Tac Toe Game",
      "description": "A modern Tic Tac Toe game built with v0.",
      "files": [
        {
          "path": "components/game-board.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/game-screen.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/menu-screen.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/online-setup-screen.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/waiting-room-screen.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/theme-provider.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/theme-toggle.tsx",
          "type": "registry:component"
        },
        {
          "path": "components/footer.tsx",
          "type": "registry:component"
        },
        {
          "path": "constants/game.ts",
          "type": "registry:lib"
        },
        {
          "path": "lib/supabase.ts",
          "type": "registry:lib"
        },
        {
          "path": "lib/utils.ts",
          "type": "registry:lib"
        },
        {
          "path": "services/online-game.ts",
          "type": "registry:lib"
        },
        {
          "path": "types/game.ts",
          "type": "registry:lib"
        },
        {
          "path": "utils/game-logic.ts",
          "type": "registry:lib"
        }
      ],
      "dependencies": [
        "@supabase/supabase-js",
        "next-themes",
        "lucide-react",
        "clsx",
        "tailwind-merge"
      ]
    }
  ]
}